{
  "name": "rest-ts",
  "version": "1.0.0",
  "description": "Minimal REST API with Hono",
  "type": "module",
  "main": "dist/server.js",
  "exports": {
    ".": "./dist/server.js"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsdown",
    "start": "node dist/server.js",
    "clean": "node scripts/clean.cjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "audit": "pnpm audit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "hono": "^4.11.7",
    "hono-rate-limiter": "^0.5.3",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.2.5",
    "tsdown": "^0.20.0",
    "tsx": "^4.19.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  }
}
