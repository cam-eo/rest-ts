{
  "name": "rest-ts",
  "version": "1.0.0",
  "description": "Minimal REST API with Fastify",
  "type": "module",
  "main": "dist/server.mjs",
  "exports": {
    ".": "./dist/server.mjs"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsdown",
    "start": "node dist/server.mjs",
    "clean": "node scripts/clean.cjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "audit": "pnpm audit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.0.0",
    "debug": "4.3.4",
    "fastify": "^5.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.2.5",
    "tsdown": "^0.20.0",
    "tsx": "^4.19.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "overrides": {
      "debug": "4.3.4"
    }
  }
}
